{
    "name": "david-moreno-cortina/blue-hole",
    "description": "A small framework based on custom libraries",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "David Moreno Cortina",
            "email": "davidmorenocortina@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DavidMorenoCortina/dependency-container.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DavidMorenoCortina/jwt.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DavidMorenoCortina/router.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DavidMorenoCortina/blue-orm.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DavidMorenoCortina/input-validation.git"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "ext-json": "*",
        "david-moreno-cortina/dependency-container": "^1.0",
        "david-moreno-cortina/jwt": "^1.0",
        "david-moreno-cortina/router": "^1.2",
        "david-moreno-cortina/blue-orm": "^1.1",
        "david-moreno-cortina/input-validation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DavidMorenoCortina\\BlueHole\\": "src"
        }
    }
}
